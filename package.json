{
    "name": "datapos-resources",
    "license": "ISC",
    "private": true,
    "version": "0.1.394",
    "devDependencies": {
        "@datapos/datapos-operations": "^0.1.339",
        "prettier": "^3.0.3"
    },
    "scripts": {
        "audit": "npm audit",
        "build": "npm run buildApplicationIndex && npm run buildFileStoreIndex",
        "buildApplicationIndex": "node -e \"require('@datapos/datapos-operations/scriptHelpers').buildPublicDirectoryIndex('application')\"",
        "buildFileStoreIndex": "node -e \"require('@datapos/datapos-operations/scriptHelpers').buildPublicDirectoryIndex('fileStore')\"",
        "bumpVersion": "node -e \"require('@datapos/datapos-operations/scriptHelpers').bumpVersion()\"",
        "check": "npx npm-check-updates && npm outdated",
        "document": "npx npm-license-crawler --onlyDirectDependencies --production --relativeLicensePath --json LICENSES.json",
        "format": "echo \"***** FORMAT SCRIPT NOT IMPLEMENTED. *****\"",
        "lint": "echo \"***** LINT SCRIPT NOT IMPLEMENTED. *****\"",
        "publishToNPM": "echo \"***** PUBLISH TO NPM SCRIPT NOT IMPLEMENTED. *****\"",
        "release": "npm run syncWithGitHub",
        "syncWithGitHub": "npm run build && npm run bumpVersion && node -e \"require('@datapos/datapos-operations/scriptHelpers').syncWithGitHub()\"",
        "test": "echo \"***** TEST SCRIPT NOT IMPLEMENTED. *****\"",
        "updateDependencies": "npm run updateOperations",
        "updateOperations": "npm install --save-dev @datapos/datapos-operations@latest"
    }
}